fi
    if test "${BUILD_WARNINGS}" = "Yes"
then :
  BUILD_WARNINGS="Yes (NOT INTENDED FOR PRODUCTION USAGE)"
fi
    if test "${COMPILER_SANITIZERS}" = "Yes"
then :
  COMPILER_SANITIZERS="Yes (${COMPILER_SANITIZERS_ENABLED})"
fi
    if test "x${USE_NLS}" = "xyes"
then :
  USE_NLS="Yes"
else $as_nop
  USE_NLS="No"
fi

    if test "x${FEATURE_SASL_ECDH_X25519_CHALLENGE}" != "xYes"
then :
  FEATURE_SASL_ECDH_X25519_CHALLENGE="No"
fi
    if test "x${FEATURE_SASL_ECDSA_NIST256P_CHALLENGE}" != "xYes"
then :
  FEATURE_SASL_ECDSA_NIST256P_CHALLENGE="No"
fi
    if test "x${FEATURE_SASL_SCRAM}" != "xYes"
then :
  FEATURE_SASL_SCRAM="No"
