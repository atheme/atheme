     PKG_CONFIG_MIN_VERSION=0.9.0
     if $PKG_CONFIG --atleast-pkgconfig-version $PKG_CONFIG_MIN_VERSION; then
        { echo "$as_me:$LINENO: checking for libmowgli >= 0.7.0" >&5
echo $ECHO_N "checking for libmowgli >= 0.7.0... $ECHO_C" >&6; }

        if $PKG_CONFIG --exists "libmowgli >= 0.7.0" ; then
            { echo "$as_me:$LINENO: result: yes" >&5
echo "${ECHO_T}yes" >&6; }
            succeeded=yes

            { echo "$as_me:$LINENO: checking MOWGLI_CFLAGS" >&5
echo $ECHO_N "checking MOWGLI_CFLAGS... $ECHO_C" >&6; }
            MOWGLI_CFLAGS=`$PKG_CONFIG --cflags "libmowgli >= 0.7.0"`
            { echo "$as_me:$LINENO: result: $MOWGLI_CFLAGS" >&5
echo "${ECHO_T}$MOWGLI_CFLAGS" >&6; }

            { echo "$as_me:$LINENO: checking MOWGLI_LIBS" >&5
echo $ECHO_N "checking MOWGLI_LIBS... $ECHO_C" >&6; }
            MOWGLI_LIBS=`$PKG_CONFIG --libs "libmowgli >= 0.7.0"`
            { echo "$as_me:$LINENO: result: $MOWGLI_LIBS" >&5
echo "${ECHO_T}$MOWGLI_LIBS" >&6; }
        else
	    { echo "$as_me:$LINENO: result: no" >&5
echo "${ECHO_T}no" >&6; }
            MOWGLI_CFLAGS=""
            MOWGLI_LIBS=""
            ## If we have a custom action on failure, don't print errors, but
            ## do set a variable so people can do so.
            MOWGLI_PKG_ERRORS=`$PKG_CONFIG --errors-to-stdout --print-errors "libmowgli >= 0.7.0"`

        fi



     else
        echo "*** Your version of pkg-config is too old. You need version $PKG_CONFIG_MIN_VERSION or newer."
        echo "*** See http://www.freedesktop.org/software/pkgconfig"
     fi
  fi

  if test $succeeded = yes; then
     MOWGLI_SOURCE="system"
  else

	MOWGLI_SOURCE="internal"
	MOWGLI_CFLAGS="-I../libmowgli/src/libmowgli -I../../libmowgli/src/libmowgli -I../../../libmowgli/src/libmowgli"
	MOWGLI_LIBS="-L../libmowgli/src/libmowgli -lmowgli"
	LIBMOWGLI="libmowgli"
	subdirs="$subdirs libmowgli"


  fi


