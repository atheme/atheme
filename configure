        ax_msg="=== configuring in $ax_dir ($(pwd)/$ax_dir)"
        printf "%s\n" "$as_me:${as_lineno-$LINENO}: $ax_msg" >&5
        printf "%s\n" "$ax_msg" >&6
        as_dir="$ax_dir"; as_fn_mkdir_p
        ac_builddir=.

