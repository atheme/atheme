/* ALIS configuration.
 *
 * ALIS provides a more flexible way to list channels.
 */
alis {

	nick = "ALIS";
	user = "alis";
	host = "services.int";
	real = "Channel Directory";

	aliases {
	};

	access {
	};

	/* (*) maxmatches
	 *
	 * The default maximum number of channels returned in a query.
